{
  "name": "cbor-utils",
  "version": "0.10.12",
  "description": "CBOR encoder and decoder classes. Analogue to minicbor from Rust",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest",
    "build": "tsup && tsc",
    "lint": "npm run build && npx size-limit",
    "prepublish": "npm run lint && npm run build",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whiteand/cbor-utils.git"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "7 kB"
    }
  ],
  "files": [
    "dist"
  ],
  "keywords": [
    "cbor",
    "encoder",
    "decoder",
    "format"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "author": "andrewbeletskiy",
  "license": "ISC",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "resultra": "^0.1.4"
  }
}
